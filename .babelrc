{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        },
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-modules-commonjs",
      {
        "loose": true,
        "strict": true,
        "noInterop": true,
        "lazy": true
      }
    ],
    ["@babel/plugin-proposal-class-properties"],
    ["inline-json-import"]
  ]
}